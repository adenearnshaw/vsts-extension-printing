{
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "arrow-return-shorthand": true,
        "callable-types": true,
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "curly": true,
        "deprecation": {
            "severity": "warn"
        },
        "eofline": true,
        "import-spacing": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-line-length": [
            true,
            240
        ],
        "no-console": [
            false,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-debugger": false,
        "no-trailing-whitespace": true,
        "no-unnecessary-initializer": true,
        "no-var-keyword": true,
        "prefer-const": true,
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ]
    },
    "rulesDirectory": []
}